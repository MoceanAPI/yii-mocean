{
  "name": "mocean/yii-mocean",
  "description": "Mocean API for Yii Framework",
  "type": "library",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "keywords": [
    "sms",
    "verify",
    "yii",
    "yiiframework",
    "mocean"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "mocean/client": "^2.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "YiiMocean\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YiiMocean\\Tests\\": "tests/"
    }
  }
}
